{
    "name": "toolkito/larasap",
    "description": "Laravel social auto posting (Larasap) lets you automatically post all your content to social networks such as Telegram Channel, X (Twitter), and Facebook.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "social network",
        "telegram",
        "twitter",
        "x",
        "facebook"
    ],
    "authors": [
        {
            "name": "Ali Hesari",
            "email": "alihesari.com@gmail.com",
            "homepage": "https://alihesari.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/support": "^12.0",
        "illuminate/contracts": "^12.0",
        "facebook/php-business-sdk": "^18.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Toolkito\\Larasap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toolkito\\Larasap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Toolkito\\Larasap\\LarasapServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
