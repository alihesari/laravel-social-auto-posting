        "mockery/mockery": "^1.3"
    },
    "require": {
        "facebook/graph-sdk": "^5.6.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Askancy\\Larasap\\LarasapServiceProvider"
            ]
        }
