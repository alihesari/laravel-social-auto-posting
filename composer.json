{
    "name": "toolkito/larasap",
    "description": "Laravel social auto posting (Larasap) lets you automatically post all your content to social networks such as Telegram Channel, X (Twitter), and Facebook.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "social network",
        "telegram",
        "twitter",
        "x",
        "facebook"
    ],
    "authors": [
        {
            "name": "Ali Hesari",
            "email": "alihesari.com@gmail.com",
            "homepage": "https://alihesari.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "dg/twitter-php": "^5.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Toolkito\\Larasap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toolkito\\Larasap\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Toolkito\\Larasap\\LarasapServiceProvider"
            ],
            "aliases": {
                "X": "Toolkito\\Larasap\\Facades\\X"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
